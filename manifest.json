{
    "manifest_version": 2,
    "name": "Gmail Domain Age Checker",
    "version": "0.3",
  
    "description": "Checks for the age of domains that email you.",
    "permissions": ["tabs", "webRequest", "<all_urls>"],
  
    "content_scripts": [
      {
        "matches": ["*://mail.google.com/*"],
        "js": ["agecheck.js"],
        "run_at": "document_end"
      }
    ]
  }
  